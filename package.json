{
  "private": true,
  "packageManager": "yarn@4.1.1",
  "workspaces": [
    "packages/*"
  ],
  "version": "1.0.0",
  "scripts": {
    "build": "yarn workspaces foreach --all --parallel run build",
    "update-versions": "yarn workspaces foreach --all --parallel version $npm_package_version",
    "version": "yarn update-versions && yarn build"
  },
  "devDependencies": {
    "esbuild": "^0.20.2"
  }
}
