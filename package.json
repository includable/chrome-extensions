{
  "private": true,
  "packageManager": "yarn@4.1.1",
  "workspaces": [
    "packages/*"
  ],
  "version": "1.0.0",
  "scripts": {
    "build": "yarn workspaces foreach --all --parallel run build",
    "postversion": "yarn workspaces foreach --all --parallel version --new-version $npm_package_version"
  },
  "devDependencies": {
    "esbuild": "^0.20.2"
  }
}
