{
  "name": "@includable-chrome/gmail-omnifocus",
  "version": "2.0.0",
  "description": "Add to OmniFocus for Gmail.",
  "main": "index.js",
  "repository": "https://github.com/tschoffelen/omnifocus-gmail.git",
  "author": "Thomas Schoffelen <thomas@schof.co>",
  "license": "MIT",
  "scripts": {
    "bundle": "esbuild index.js --bundle --minify --outfile=dist/index.js",
    "build": "yarn bundle && rm -rf dist/extension.zip && zip -r dist/extension.zip dist resources",
    "dev": "yarn bundle --watch",
    "prepare": "yarn build"
  },
  "dependencies": {
    "@includable-chrome/shared": "workspace:^"
  }
}
